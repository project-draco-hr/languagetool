{
  UnpairedQuotesBracketsRule rule=new UnpairedQuotesBracketsRule(TestTools.getEnglishMessages(),Language.ROMANIAN);
  RuleMatch[] matches;
  JLanguageTool langTool=new JLanguageTool(Language.ROMANIAN);
  matches=rule.match(langTool.getAnalyzedSentence("A fost plecat (pentru pu??in timp)."));
  assertEquals(0,matches.length);
  matches=rule.match(langTool.getAnalyzedSentence("Nu's de prin locurile astea."));
  assertEquals(0,matches.length);
  matches=rule.match(langTool.getAnalyzedSentence("A fost plecat pentru ???pu??in timp???."));
  assertEquals(0,matches.length);
  matches=rule.match(langTool.getAnalyzedSentence("A fost plecat ???pentru... pu??in timp???."));
  assertEquals(0,matches.length);
  matches=rule.match(langTool.getAnalyzedSentence("A fost plecat ???pentru... ??pu??in?? timp???."));
  assertEquals(0,matches.length);
  matches=rule.match(langTool.getAnalyzedSentence("A fost plecat \"pentru pu??in timp."));
  assertEquals(0,matches.length);
  matches=rule.match(langTool.getAnalyzedSentence("A fost plecat ???pentru... pu??in timp."));
  assertEquals(1,matches.length);
  matches=rule.match(langTool.getAnalyzedSentence("A fost plecat ??pu??in."));
  assertEquals(1,matches.length);
  matches=rule.match(langTool.getAnalyzedSentence("A fost plecat ???pentru ??pu??in timp???."));
  assertEquals(1,matches.length);
  matches=rule.match(langTool.getAnalyzedSentence("A fost plecat ???pentru pu??in?? timp???."));
  assertEquals(1,matches.length);
  matches=rule.match(langTool.getAnalyzedSentence("A fost plecat ???pentru... pu??in?? timp???."));
  assertEquals(1,matches.length);
}
