{
  if (rulesBox.getSelectedIndex() == -1) {
    coveredByPane.setText("");
  }
 else {
    if (coveredByList != null) {
      int index=getCurrentRuleIndex();
      if (!coveredByList[index].isEmpty()) {
        coveredByPane.setText(coveredByList[index]);
        coveredByPane.repaint();
      }
 else {
        coveredByPane.setText("");
        coveredByPane.repaint();
      }
    }
  }
}
