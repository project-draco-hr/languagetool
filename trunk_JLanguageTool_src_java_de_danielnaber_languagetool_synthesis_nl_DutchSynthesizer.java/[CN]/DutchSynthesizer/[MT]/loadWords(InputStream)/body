{
  final ArrayList<String> set=new ArrayList<String>();
  InputStreamReader isr=null;
  BufferedReader br=null;
  try {
    isr=new InputStreamReader(file);
    br=new BufferedReader(isr);
    String line;
    while ((line=br.readLine()) != null) {
      line=line.trim();
      if (line.length() < 1) {
        continue;
      }
      if (line.charAt(0) == '#') {
        continue;
      }
      set.add(line);
    }
  }
  finally {
    if (br != null) {
      br.close();
    }
    if (isr != null) {
      isr.close();
    }
  }
  return set;
}
