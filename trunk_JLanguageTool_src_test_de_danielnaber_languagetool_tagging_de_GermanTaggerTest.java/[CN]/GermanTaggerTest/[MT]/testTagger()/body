{
  GermanTagger tagger=new GermanTagger();
  AnalyzedGermanTokenReadings aToken=tagger.lookup("Haus");
  assertEquals("Haus[Nomen/Akk/Sin/Neu, Nomen/Dat/Sin/Neu, Nomen/Nom/Sin/Neu]",aToken.toString());
  aToken=tagger.lookup("Hauses");
  assertEquals("Hauses[Nomen/Gen/Sin/Neu]",aToken.toString());
  aToken=tagger.lookup("hauses");
  assertNull(aToken);
  aToken=tagger.lookup("Gro??");
  assertNull(aToken);
  aToken=tagger.lookup("gro??er");
  assertEquals("gro??er[Adjektiv/Dat/Sin/Fem, Adjektiv/Gen/Plu/Fem, " + "Adjektiv/Gen/Plu/Mas, Adjektiv/Gen/Plu/Neu, " + "Adjektiv/Gen/Sin/Fem, Adjektiv/Nom/Sin/Mas]",aToken.toString());
}
