{
  GermanTagger tagger=new GermanTagger();
  AnalyzedGermanTokenReadings aToken=tagger.lookup("Haus");
  assertEquals("Haus[SUB:AKK:SIN:NEU, SUB:DAT:SIN:NEU, SUB:NOM:SIN:NEU]",aToken.toString());
  assertEquals("Haus",aToken.getReadings().get(0).getLemma());
  assertEquals("Haus",aToken.getReadings().get(1).getLemma());
  assertEquals("Haus",aToken.getReadings().get(2).getLemma());
  aToken=tagger.lookup("Hauses");
  assertEquals("Hauses[SUB:GEN:SIN:NEU]",aToken.toString());
  assertEquals("Haus",aToken.getReadings().get(0).getLemma());
  aToken=tagger.lookup("hauses");
  assertNull(aToken);
  aToken=tagger.lookup("Gro??");
  assertNull(aToken);
  aToken=tagger.lookup("gro??er");
  assertEquals("gro??er[ADJ:DAT:SIN:FEM:GRU:SOL, ADJ:GEN:PLU:FEM:GRU:SOL, " + "ADJ:GEN:PLU:MAS:GRU:SOL, ADJ:GEN:PLU:NEU:GRU:SOL, " + "ADJ:GEN:SIN:FEM:GRU:SOL, ADJ:NOM:SIN:MAS:GRU:IND, ADJ:NOM:SIN:MAS:GRU:SOL]",aToken.toString());
  assertEquals("gro??",aToken.getReadings().get(0).getLemma());
  aToken=tagger.lookup("Interessen");
  assertEquals("Interessen[SUB:DAT:PLU:NEU, SUB:NOM:PLU:NEU, SUB:AKK:PLU:NEU, SUB:GEN:PLU:NEU]",aToken.toString());
  assertEquals("Interesse",aToken.getReadings().get(0).getLemma());
  assertEquals("Interesse",aToken.getReadings().get(1).getLemma());
  assertEquals("Interesse",aToken.getReadings().get(2).getLemma());
  assertEquals("Interesse",aToken.getReadings().get(3).getLemma());
  aToken=tagger.lookup("Donaudampfschiff");
  assertEquals("Donaudampfschiff[SUB:AKK:SIN:NEU, SUB:DAT:SIN:NEU, SUB:NOM:SIN:NEU]",aToken.toString());
  assertEquals("Donaudampfschiff",aToken.getReadings().get(0).getLemma());
  assertEquals("Donaudampfschiff",aToken.getReadings().get(1).getLemma());
  aToken=tagger.lookup("H??userk??mpfe");
  assertEquals("H??userk??mpfe[SUB:AKK:PLU:MAS, SUB:GEN:PLU:MAS, SUB:NOM:PLU:MAS]",aToken.toString());
  assertEquals("H??userkampf",aToken.getReadings().get(0).getLemma());
  assertEquals("H??userkampf",aToken.getReadings().get(1).getLemma());
  assertEquals("H??userkampf",aToken.getReadings().get(2).getLemma());
  aToken=tagger.lookup("H??userkampfes");
  assertEquals("H??userkampfes[SUB:GEN:SIN:MAS]",aToken.toString());
  assertEquals("H??userkampf",aToken.getReadings().get(0).getLemma());
  aToken=tagger.lookup("H??userkampfs");
  assertEquals("H??userkampfs[SUB:GEN:SIN:MAS]",aToken.toString());
  assertEquals("H??userkampf",aToken.getReadings().get(0).getLemma());
  aToken=tagger.lookup("Lieblingsfarben");
  assertEquals("Lieblingsfarben[SUB:AKK:PLU:FEM, SUB:DAT:PLU:FEM, SUB:GEN:PLU:FEM, " + "SUB:NOM:PLU:FEM]",aToken.toString());
  assertEquals("Lieblingsfarbe",aToken.getReadings().get(0).getLemma());
  aToken=tagger.lookup("Autolieblingsfarben");
  assertEquals("Autolieblingsfarben[SUB:AKK:PLU:FEM, SUB:DAT:PLU:FEM, SUB:GEN:PLU:FEM, " + "SUB:NOM:PLU:FEM]",aToken.toString());
  assertEquals("Autolieblingsfarbe",aToken.getReadings().get(0).getLemma());
}
