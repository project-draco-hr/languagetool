{
  GermanTagger tagger=new GermanTagger();
  AnalyzedGermanToken aToken=tagger.lookup("Haus",0);
  assertEquals("Haus[Nomen/Nom/Sin/Neu, Nomen/Dat/Sin/Neu, " + "Nomen/Akk/Sin/Neu]",aToken.toString());
  aToken=tagger.lookup("Hauses",0);
  assertEquals("Hauses[Nomen/Gen/Sin/Neu]",aToken.toString());
  aToken=tagger.lookup("hauses",0);
  assertNull(aToken);
  aToken=tagger.lookup("Gro??",0);
  assertNull(aToken);
  aToken=tagger.lookup("gro??er",0);
  assertEquals("gro??er[Adjektiv/Nom/Sin/Mas, Adjektiv/Gen/Plu/Mas, " + "Adjektiv/Gen/Sin/Fem, Adjektiv/Dat/Sin/Fem, " + "Adjektiv/Gen/Plu/Fem, Adjektiv/Gen/Plu/Neu]",aToken.toString());
}
