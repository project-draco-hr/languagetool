{
  final ArrayList<String> set=new ArrayList<>();
  final Scanner scanner=new Scanner(stream,"UTF-8");
  try {
    while (scanner.hasNextLine()) {
      final String line=scanner.nextLine().trim();
      if (line.length() < 1 || line.charAt(0) == '#') {
        continue;
      }
      set.add(line);
    }
  }
  finally {
    scanner.close();
  }
  return set;
}
