{
  char d=(char)0;
  while ((nindex < inArray.length) && inArray[nindex] != a && inArray[nindex] != b) {
    if (ISNL(nindex)) {
      ++d;
    }
    if (inArray[nindex] == '#' && !ISESC(nindex)) {
      d+=SKIPLN_N();
      nindex--;
    }
    if (!ISSPACE(nindex)) {
      break;
    }
    ++nindex;
  }
  return d;
}
