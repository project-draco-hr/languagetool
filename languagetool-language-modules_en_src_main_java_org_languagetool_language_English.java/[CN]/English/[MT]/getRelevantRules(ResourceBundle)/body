{
  return Arrays.asList(new CommaWhitespaceRule(messages),new DoublePunctuationRule(messages),new UppercaseSentenceStartRule(messages,this),new MultipleWhitespaceRule(messages,this),new LongSentenceRule(messages),new SentenceWhitespaceRule(messages),new EnglishUnpairedBracketsRule(messages,this),new EnglishWordRepeatRule(messages,this),new AvsAnRule(messages),new EnglishWordRepeatBeginningRule(messages,this),new CompoundRule(messages),new ContractionSpellingRule(messages));
}
