{
  text=text.replaceAll("(?s)<header.*?>.*?</header>","");
  text=text.replaceAll("<w.*?>","");
  text=text.replaceAll("<c.*?>","");
  text=text.replaceAll("<.*?>","");
  text=text.replaceAll(" +"," ");
  text=text.replaceAll("&bquo|&equo","\"");
  text=text.replaceAll("&mdash;?","--");
  text=text.replaceAll("&amp;?","&");
  return text;
}
