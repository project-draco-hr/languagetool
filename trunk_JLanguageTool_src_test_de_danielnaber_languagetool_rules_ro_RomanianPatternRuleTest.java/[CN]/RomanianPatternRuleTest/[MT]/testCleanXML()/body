{
  assertEquals(cleanXML("1<mark>2"),"12");
  assertEquals(cleanXML("1</mark>2"),"12");
  assertEquals(cleanXML("1<</mark>2"),"1<2");
  assertEquals(cleanXML("<</mark>2"),"<2");
  assertEquals(cleanXML("></mark>2"),">2");
  assertEquals(cleanXML("1<mark>abc</mark>2"),"1abc2");
  assertEquals(cleanXML("1<mark><<</mark>2"),"1<<2");
  assertEquals(cleanXML("1<mark>>></mark>2"),"1>>2");
}
