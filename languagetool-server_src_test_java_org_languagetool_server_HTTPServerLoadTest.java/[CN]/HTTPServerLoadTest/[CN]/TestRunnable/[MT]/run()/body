{
  for (int i=0; i < getRepeatCount(); i++) {
    runningTests.incrementAndGet();
    try {
      runTestsV1();
      runTestsV2();
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
 finally {
      int count=runningTests.decrementAndGet();
      System.out.println("Tests currently running: " + count);
    }
  }
}
