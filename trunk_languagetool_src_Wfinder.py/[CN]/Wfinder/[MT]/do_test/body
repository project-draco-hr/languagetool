def do_test(self, l):
    if (l == ''):
        return ''
    else:
        oldword = l
        found = 0
        retval = self.suffix_search(l, '', '')
        if (retval != ''):
            found = 1
            return retval
        retval = self.prefix_search(l)
        if (retval != ''):
            found = 1
            return retval
        if self.is_secondflag:
            retval = self.suffix22_search(l, '', '')
            if (retval != ''):
                found = 1
                return retval
            retval = self.prefix22_search(l)
            if (retval != ''):
                found = 1
                return retval
        return ('- %s' % oldword)
