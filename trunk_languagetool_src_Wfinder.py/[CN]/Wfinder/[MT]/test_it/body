def test_it(self, l):
    if (self.is_initialized == 0):
        self.aff_read()
        self.dic_read()
        self.is_initialized = 1
    lcasetest = 0
    result = self.do_keytest(l)
    if (result[0] == '-'):
        lu = l[0]
        if (lu != lu.lower()):
            l1 = (lu[0].lower() + l[1:])
            if (l1 != l):
                lcasetest = 1
                result = self.do_keytest(l1)
                if ((result[0] == '-') and (self.textlanguage != 'de')):
                    tmp = l1
                    l1 = l
                    l = tmp
    if (result[0] == '-'):
        result = self.do_test(l)
    if ((result[0] == '-') and (lcasetest == 1)):
        result = self.do_test(l1)
    typ = ''
    if (result[0] != '-'):
        src = result.split()
        word = src[(len(src) - 1)]
        oword = src[1]
        typ = typdic[word]
        if (self.textlanguage == 'de'):
            if (typ != ''):
                if ((typ == 'V') or (typ == 'HV')):
                    if ((oword[(-4):] == 'ende') or (oword[(-5):(-1)] == 'ende')):
                        typ = 'ADJV'
                if ((typ == 'V') or (typ == 'HV')):
                    if (oword[(-1):] == 'e'):
                        typ = (typ + '11')
                    elif (oword[(-2):] == 'st'):
                        typ = (typ + '12')
                    elif (oword[(-2):] == 'en'):
                        typ = (typ + '14')
                    elif (oword[(-2):] == 'et'):
                        typ = (typ + '15')
                    elif (oword[(-1):] == 't'):
                        typ = (typ + '13')
                elif (typ == 'ADJ'):
                    if (oword[(-2):] == 'er'):
                        typ = 'ADJER'
                    elif (oword[(-2):] == 'en'):
                        typ = 'ADJEN'
                    elif (oword[(-2):] == 'em'):
                        typ = 'ADJEM'
                    elif (oword[(-2):] == 'es'):
                        typ = 'ADJES'
                    elif (oword[(-1):] == 'e'):
                        typ = 'ADJE'
                elif (typ == 'NMS'):
                    if (oword[(-2):] == 'in'):
                        typ = 'NFS'
                    elif (oword[(-5):] == 'innen'):
                        typ = 'NF'
                if (typ[0] == 'N'):
                    if ((word != oword) and (typ[(-1):] == 'S')):
                        typ = typ[0:(-1)]
        elif (self.textlanguage == 'hu'):
            dif = (len(oword) - len(word))
            if (((typ[0] == 'V') or (typ[0:2] == 'SI')) and (word != oword)):
                ik = ''
                telo = 'SI'
                if (typ[0] == 'V'):
                    telo = 'V'
                if (oword[0:2] != word[0:2]):
                    ik = 'IK'
                if ((oword[(-3):] in (u'i\xfck', 'iuk', 'nak', 'nek', 'tak', 'tek')) or (oword[(-2):] in (u'\xe1k', u'\xe9k'))):
                    typ = ((ik + telo) + '6')
                elif (oword[(-3):] in ('tok', 'tek', u't\xf6k')):
                    typ = ((ik + telo) + '5')
                elif ((oword[(-3):] in (u'\xfcnk', 'unk', u'\xe1nk', u'\xe9nk')) or (oword[(-2):] in ('uk', u'\xfck'))):
                    typ = ((ik + telo) + '4')
                elif (oword[(-2):] in ('sz', 'od', 'ed', u'\xf6d', u'\xe9d', 'ad', u'\xe1d')):
                    typ = ((ik + telo) + '2')
                elif (oword[(-2):] in ('ok', 'ek', u'\xf6k', 'om', 'em', u'\xf6m', u'\xe1m', u'\xe9m', 'am')):
                    typ = ((ik + telo) + '1')
                elif ((oword[(-2):] in ('va', 've')) or (oword[(-3):] in (u'v\xe1n', u'v\xe9n'))):
                    typ = 'ADV'
                elif (oword[(-2):] == 'ni'):
                    typ = 'INF'
                else:
                    typ = ((ik + telo) + '3')
            elif (typ[0:3] == 'PP4'):
                if (oword != 'mi'):
                    typ = 'ADV'
            elif (typ[0:3] == 'ADJ'):
                if ((oword[(-2):] in ('ek', 'ok', 'ak', u'\xe9k', u'\xe1k')) and (dif > 0) and ((dif < 3) or ((word[0:1] != oword[0:1]) and (dif < 9)))):
                    typ = 'ADJP'
                elif ((oword[(-1):] in (u'\xe9', u'\xe1')) and (dif > 0) and ((dif < 5) or ((word[0:1] != oword[0:1]) and (dif < 12)))):
                    typ = 'ADV'
                elif ((oword[(-2):] in ('an', 'en', 'bb', 'ul', u'\xfcl')) and (dif == 2)):
                    typ = 'ADV'
                elif (dif != 0):
                    typ = 'ADV'
            elif (typ[0] == 'N'):
                if ((oword[(-2):] in ('ek', 'ok', 'ak', u'\xe9k', u'\xe1k', u'\xf3k', u'\xfbk', u'\xf5k', u'\xfak')) and (dif > 0) and (dif < 3)):
                    typ = 'NP'
                elif ((oword[(-1):] == 'i') and (dif == 1)):
                    typ = 'DNA'
                elif (((oword[(-1):] in (u'\xfa', u'\xfb')) and (dif == 1)) or ((oword[(-2):] in (u'j\xfa', u'j\xfb')) and (dif == 2))):
                    typ = 'ADJS'
                elif (typ == 'N'):
                    if ((oword[(-1)] == 'k') and (oword == word)):
                        typ = 'NP'
                    else:
                        typ = 'NS'
                elif (dif >= 2):
                    typ = 'N'
            if ((typ[0] == 'N') and (oword == word) and (word[(-1)] != 'k')):
                typ = (typ + 'N')
        result = ((result + ' ') + typ)
    return result
