{
  List<Span> spans=new ArrayList<>();
  List<RuleMatch> matches=new ArrayList<>();
  for (  RuleMatch match : ruleMatches) {
    if (match.getRule().getId().equals(disabledRule)) {
      continue;
    }
    matches.add(match);
    createSpan(spans,match);
  }
  prepareUpdateHighlights(matches,spans);
}
