{
  if (ruleMatchArray[i] != 0 && isInMatches(ruleMatchArray[i] - 1)) {
    setAsDeleted(ruleMatchArray[i] - 1);
    ruleMatchArray[i]=0;
  }
 else {
    ruleMatchIndex++;
    ruleMatchArray[i]=ruleMatchIndex;
    return new RuleMatch(this,startPos,startPos + 1,messages.getString("unpaired_brackets"));
  }
  return null;
}
