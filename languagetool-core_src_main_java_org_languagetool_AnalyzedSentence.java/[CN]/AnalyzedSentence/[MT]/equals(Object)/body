{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  final AnalyzedSentence other=(AnalyzedSentence)obj;
  if (!Arrays.equals(nonBlankTokens,other.nonBlankTokens))   return false;
  if (!Arrays.equals(tokens,other.tokens))   return false;
  if (!Arrays.equals(whPositions,other.whPositions))   return false;
  if (tokenSet != null && !tokenSet.equals(other.tokenSet)) {
    return false;
  }
  if (lemmaSet != null && !lemmaSet.equals(other.lemmaSet)) {
    return false;
  }
  return true;
}
