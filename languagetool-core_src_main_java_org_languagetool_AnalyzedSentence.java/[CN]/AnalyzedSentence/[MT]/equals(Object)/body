{
  if (this == o)   return true;
  if (o == null)   return false;
  if (getClass() != o.getClass())   return false;
  AnalyzedSentence other=(AnalyzedSentence)o;
  return Arrays.equals(nonBlankTokens,other.nonBlankTokens) && Arrays.equals(tokens,other.tokens) && Arrays.equals(whPositions,other.whPositions);
}
