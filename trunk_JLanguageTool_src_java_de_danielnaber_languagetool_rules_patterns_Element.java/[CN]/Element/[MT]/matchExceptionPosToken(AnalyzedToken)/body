{
  if (exceptionPosToken == null) {
    return true;
  }
  boolean match=false;
  if (!exceptionPosRegExp) {
    if (exceptionPosToken.equals(token.getPOSTag())) {
      match=true;
    }
  }
 else   if (token.getPOSTag() != null) {
    if (Pattern.matches(exceptionPosToken,token.getPOSTag())) {
      match=true;
    }
  }
  return match;
}
