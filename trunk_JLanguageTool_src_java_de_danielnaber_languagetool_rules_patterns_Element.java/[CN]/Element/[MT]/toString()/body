{
  final StringBuilder sb=new StringBuilder();
  if (negation) {
    sb.append("!");
  }
  sb.append(stringToken);
  if (phraseName != null) {
    sb.append(" {");
    sb.append(phraseName);
    sb.append("}");
  }
  if (posToken != null) {
    sb.append("/");
    sb.append(posToken);
  }
  return sb.toString();
}
