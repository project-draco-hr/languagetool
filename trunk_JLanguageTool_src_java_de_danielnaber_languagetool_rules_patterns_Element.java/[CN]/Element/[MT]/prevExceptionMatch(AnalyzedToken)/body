{
  boolean exceptionMatched=false;
  if (exceptionSet) {
    for (    Element testException : exceptionList) {
      if (testException.exceptionValidNext) {
        exceptionMatched|=testException.match(token);
      }
      if (exceptionMatched) {
        break;
      }
    }
  }
  return exceptionMatched;
}
