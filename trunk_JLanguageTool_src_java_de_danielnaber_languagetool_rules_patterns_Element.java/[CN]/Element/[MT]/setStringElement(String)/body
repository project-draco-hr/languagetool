{
  this.stringToken=token;
  testString=true;
  if (StringTools.isEmpty(stringToken)) {
    testString=false;
  }
  if (testString && stringRegExp) {
    regToken=stringToken;
    if (!caseSensitive) {
      regToken=CASE_INSENSITIVE + stringToken;
    }
    if (!"\\0".equals(token)) {
      p=Pattern.compile(regToken);
    }
  }
}
