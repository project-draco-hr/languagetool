{
  String testToken=null;
  if (inflected) {
    testToken=token.getLemma();
    if (testToken == null) {
      testToken=token.getToken();
    }
  }
 else {
    testToken=token.getToken();
  }
  if (stringRegExp) {
    if (testToken != null) {
      if (m == null) {
        m=p.matcher(testToken);
      }
 else {
        m.reset(testToken);
      }
      return m.matches();
    }
  }
 else {
    if (caseSensitive) {
      return stringToken.equals(testToken);
    }
 else {
      return stringToken.equalsIgnoreCase(testToken);
    }
  }
  return false;
}
