{
  if (stringToken == null) {
    return true;
  }
  if (stringToken.equals("")) {
    return true;
  }
  String testToken=null;
  if (inflected) {
    testToken=token.getLemma();
    if (testToken == null)     testToken=token.getToken();
  }
 else   testToken=token.getToken();
  String regToken=stringToken;
  if (!caseSensitive) {
    regToken="(?u)".concat(stringToken);
  }
  if (!stringRegExp) {
    if (caseSensitive) {
      return stringToken.equals(testToken);
    }
 else {
      return stringToken.equalsIgnoreCase(testToken);
    }
  }
 else {
    if (token.getToken() != null) {
      Pattern p=Pattern.compile(regToken);
      Matcher m=p.matcher(testToken);
      return m.matches();
    }
  }
  return false;
}
