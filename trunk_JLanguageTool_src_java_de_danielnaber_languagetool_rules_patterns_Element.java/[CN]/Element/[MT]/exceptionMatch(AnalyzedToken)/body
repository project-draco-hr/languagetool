{
  boolean exceptionMatched=false;
  if (exceptionSet) {
    Iterator<Element> it=exceptionList.iterator();
    while (it.hasNext()) {
      Element testException=it.next();
      exceptionMatched|=testException.match(token);
      if (exceptionMatched) {
        break;
      }
    }
  }
  return exceptionMatched;
}
