{
  boolean andGroupMatched=false;
  if (andGroupSet) {
    for (int i=0; i < andGroupList.size(); i++) {
      final Element testAndGroup=andGroupList.get(i);
      if (testAndGroup.match(token)) {
        andGroupMatched=true;
        andGroupCheck[i]=true;
      }
    }
  }
  return andGroupMatched;
}
