{
  boolean testString=true;
  if (stringToken == null) {
    testString=false;
  }
 else {
    if (stringToken.equals("")) {
      testString=false;
    }
  }
  if (testString) {
    return (matchStringToken(token) != negation) && (matchPosToken(token) != posNegation);
  }
 else {
    return (!negation) && (matchPosToken(token) != posNegation);
  }
}
