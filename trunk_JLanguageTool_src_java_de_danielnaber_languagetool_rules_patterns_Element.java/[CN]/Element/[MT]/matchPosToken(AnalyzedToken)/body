{
  if (posToken == null) {
    return true;
  }
  boolean match=false;
  if (!posRegExp) {
    match=posToken.equals(token.getPOSTag());
  }
 else   if (token.getPOSTag() != null) {
    if (mPos == null) {
      mPos=pPos.matcher(token.getPOSTag());
    }
 else {
      mPos.reset(token.getPOSTag());
    }
    match=mPos.matches();
  }
  return match;
}
