{
  return Arrays.asList(new CommaWhitespaceRule(messages),new DoublePunctuationRule(messages),new GenericUnpairedBracketsRule(messages,Arrays.asList("[","(","{","???","??","??","???","\"","'"),Arrays.asList("]",")","}","???","??","??","???","\"","'")),new HunspellRule(messages,this),new UppercaseSentenceStartRule(messages,this),new MultipleWhitespaceRule(messages,this),new SimpleReplaceRule(messages),new CastWordsRule(messages));
}
