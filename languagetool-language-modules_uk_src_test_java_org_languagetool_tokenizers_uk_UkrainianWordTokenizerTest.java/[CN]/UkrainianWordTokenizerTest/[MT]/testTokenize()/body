{
  List<String> testList=w.tokenize("???????? ?????????????? ????????????.");
  assertEquals(Arrays.asList("????????"," ","??????????????"," ","????????????","."),testList);
  testList=w.tokenize("????????\u0301 ??????\u00AD???????? ?????????????? ???????????????????.");
  assertEquals(Arrays.asList("????????"," ","??????????????"," ","??'??????????"," ","??????'??????????","."),testList);
  testList=w.tokenize("?????????????? ??.??????????????.");
  assertEquals(Arrays.asList("??????????????"," ","??",".","??????????????","."),testList);
  testList=w.tokenize("?????????????? ??.??.??????????????.");
  assertEquals(Arrays.asList("??????????????"," ","??",".","??",".","??????????????","."),testList);
  testList=w.tokenize("???????????????? 2,2 ????????????????");
  assertEquals(Arrays.asList("????????????????"," ","2,2"," ","????????????????"),testList);
}
