{
  List<String> testList=w.tokenize("???????? ?????????????? ????????????.");
  assertEquals(Arrays.asList("????????"," ","??????????????"," ","????????????","."),testList);
  testList=w.tokenize("???????? ?????????????? ?????????????? ???????????????????.");
  assertEquals(Arrays.asList("????????"," ","??????????????"," ","??'??????????"," ","??????'??????????","."),testList);
  testList=w.tokenize("?????????????? ??.??????????????.");
  assertEquals(Arrays.asList("??????????????"," ","??",".","??????????????","."),testList);
  testList=w.tokenize("?????????????? ??.??.??????????????.");
  assertEquals(Arrays.asList("??????????????"," ","??",".","??",".","??????????????","."),testList);
  testList=w.tokenize("??.\u00A0??????????????.");
  assertEquals(Arrays.asList("??",".","\u00A0","??????????????","."),testList);
  testList=w.tokenize("300 ??????. ???? ??????????????");
  assertEquals(Arrays.asList("300"," ","??????."," ","????"," ","??????????????"),testList);
  testList=w.tokenize("???????????????? 2,2 ????????????????");
  assertEquals(Arrays.asList("????????????????"," ","2,2"," ","????????????????"),testList);
  testList=w.tokenize("???????????????? 84,46 ????????????????");
  assertEquals(Arrays.asList("????????????????"," ","84,46"," ","????????????????"),testList);
  testList=w.tokenize("?????????????? 14.07.2001 ??????????");
  assertEquals(Arrays.asList("??????????????"," ","14.07.2001"," ","??????????"),testList);
  testList=w.tokenize("?????????? ?? 7.30 ??????????");
  assertEquals(Arrays.asList("??????????"," ","??"," ","7.30"," ","??????????"),testList);
  testList=w.tokenize("?? ??????????????????(????????????????");
  assertEquals(Arrays.asList("??"," ","??????????????????","(","????????????????"),testList);
  testList=w.tokenize("??????(??) ???? ????(??)??");
  assertEquals(Arrays.asList("??????(??)"," ","????"," ","????(??)??"),testList);
  testList=w.tokenize("???????????? ??... ??????????????????????");
  assertEquals(Arrays.asList("????????????"," ","??","..."," ","??????????????????????"),testList);
  testList=w.tokenize("140 ??????. ??????????????????????");
  assertEquals(Arrays.asList("140"," ","??????."," ","??????????????????????"),testList);
  testList=w.tokenize("????????. ??????????????");
  assertEquals(Arrays.asList("????????."," ","??????????????"),testList);
}
