{
  List<String> testList=w.tokenize("300 ?????? ???? ??????????????");
  assertEquals(Arrays.asList("300"," ","??????"," ","????"," ","??????????????"),testList);
  testList=w.tokenize("???????????????? 2,2 ????????????????");
  assertEquals(Arrays.asList("????????????????"," ","2,2"," ","????????????????"),testList);
  testList=w.tokenize("???????????????? 84,46 ????????????????");
  assertEquals(Arrays.asList("????????????????"," ","84,46"," ","????????????????"),testList);
  testList=w.tokenize("2 000 ?????? ?? 12 000 ??????????");
  assertEquals(Arrays.asList("2 000"," ","??????"," ","??"," ","12 000"," ","??????????"),testList);
  testList=w.tokenize("???????????????? 12 000 000 ??????");
  assertEquals(Arrays.asList("????????????????"," ","12 000 000"," ","??????"),testList);
  testList=w.tokenize("???? 01.01.42 400 000 ????.");
  assertEquals(Arrays.asList("????"," ","01.01.42"," ","400 000"," ","????."),testList);
  testList=w.tokenize("2 15 ??????????????????");
  assertEquals(Arrays.asList("2"," ","15"," ","??????????????????"),testList);
  testList=w.tokenize("?? 2004 200 ??????????????????");
  assertEquals(Arrays.asList("??"," ","2004"," ","200"," ","??????????????????"),testList);
  testList=w.tokenize("?? ??????????????-2004 200 ??????????????????");
  assertEquals(Arrays.asList("??"," ","??????????????-2004"," ","200"," ","??????????????????"),testList);
  testList=w.tokenize("?? 12 0001 ??????????");
  assertEquals(Arrays.asList("??"," ","12"," ","0001"," ","??????????"),testList);
  testList=w.tokenize("?????????????? 14.07.2001 ??????????");
  assertEquals(Arrays.asList("??????????????"," ","14.07.2001"," ","??????????"),testList);
  testList=w.tokenize("?????????? ?? 7.30 ??????????");
  assertEquals(Arrays.asList("??????????"," ","??"," ","7.30"," ","??????????"),testList);
  testList=w.tokenize("?????????? ?? 7:30 ??????????");
  assertEquals(Arrays.asList("??????????"," ","??"," ","7:30"," ","??????????"),testList);
}
