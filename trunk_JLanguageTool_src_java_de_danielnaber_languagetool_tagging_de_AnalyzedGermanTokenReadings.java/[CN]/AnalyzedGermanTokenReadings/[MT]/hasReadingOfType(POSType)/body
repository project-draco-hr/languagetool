{
  if (anTokReadings == null)   return false;
  for (  AnalyzedToken reading : anTokReadings) {
    if (reading.getPOSTag() != null) {
      if (reading.getPOSTag().equals("SENT_END") || reading.getPOSTag().equals("PARA_END")) {
        return false;
      }
    }
    AnalyzedGermanToken germanReading=(AnalyzedGermanToken)reading;
    if (germanReading.getType() == type)     return true;
  }
  return false;
}
