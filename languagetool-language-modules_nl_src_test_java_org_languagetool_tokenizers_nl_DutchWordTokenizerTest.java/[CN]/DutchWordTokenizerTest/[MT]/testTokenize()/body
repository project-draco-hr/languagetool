{
  assertTokenize("This is\u00A0a test","[This,  , is, ??, a,  , test]");
  assertTokenize("Bla bla oma's bla bla 'test","[Bla,  , bla,  , oma's,  , bla,  , bla,  , ', test]");
  assertTokenize("Ik zie het''","[Ik,  , zie,  , het, ', ']");
  assertTokenize("''Ik zie het","[', ', Ik,  , zie,  , het]");
  assertTokenize("Ik 'zie' het","[Ik,  , ', zie, ',  , het]");
  assertTokenize("'","[']");
  assertTokenize("''","[, ', ']");
  assertTokenize("'x'","[', x, ']");
}
