{
  System.out.println(sentenceCount + " lines checked.");
  System.out.println(goodMatches + " errors found that are marked as errors in the corpus " + "(this does not count whether LanguageTool's correction was perfect)");
  float recall=(float)goodMatches / sentenceCount * 100;
  System.out.printf(" => %.2f%% recall\n",recall);
}
