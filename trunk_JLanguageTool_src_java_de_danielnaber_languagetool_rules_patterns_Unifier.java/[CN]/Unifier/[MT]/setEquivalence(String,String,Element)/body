{
  if (equivalenceTypes == null) {
  }
  if (equivalenceFeatures == null) {
  }
  if (equivalenceTypes.containsKey(feature + ":" + type)) {
  }
  equivalenceTypes.put(feature + ":" + type,elem);
  List<String> lTypes;
