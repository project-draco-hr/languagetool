{
  if (inUnification) {
    uniMatched|=isSatisfied(matchToken,feature,type);
    uniAllMatched=uniMatched;
    if (lastReading) {
      uniAllMatched&=uniMatched;
      startNextToken();
      unifiedTokens=getUnifiedTokens();
      uniMatched=false;
    }
    return uniAllMatched;
  }
  if (isUniNegated) {
    setNegation(true);
  }
  isSatisfied(matchToken,feature,type);
  if (lastReading) {
    inUnification=true;
    uniMatched=false;
    startUnify();
  }
  return true;
}
