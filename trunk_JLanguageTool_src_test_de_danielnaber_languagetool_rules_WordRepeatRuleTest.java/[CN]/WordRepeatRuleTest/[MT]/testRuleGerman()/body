{
  WordRepeatRule rule=new WordRepeatRule(Language.GERMAN);
  RuleMatch[] matches;
  JLanguageTool langTool=new JLanguageTool(Language.GERMAN);
  matches=rule.match(langTool.getAnalyzedSentence("Das sind die S??tze, die die testen sollen."));
  assertEquals(0,matches.length);
  matches=rule.match(langTool.getAnalyzedSentence("Die die S??tze zum testen."));
  assertEquals(1,matches.length);
}
