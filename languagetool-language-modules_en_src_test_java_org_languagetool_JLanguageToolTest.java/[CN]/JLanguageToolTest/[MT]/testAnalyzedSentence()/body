{
  final JLanguageTool tool=new JLanguageTool(new English());
  assertEquals("<S> This[this/DT/B-NP-singular,E-NP-singular] is[be/VBZ/B-VP] a[a/DT/B-NP-singular] " + "test??ed[tested/JJ/I-NP-singular,test/VBD/I-NP-singular,test/VBN/I-NP-singular,test??ed/null] " + "sentence[sentence/NN/E-NP-singular,sentence/VB/E-NP-singular,sentence/VBP/E-NP-singular].[././O,</S>]",tool.getAnalyzedSentence("This is a test\u00aded sentence.").toString());
  assertEquals("<S> </S><P/> ",tool.getAnalyzedSentence("\n").toString());
}
