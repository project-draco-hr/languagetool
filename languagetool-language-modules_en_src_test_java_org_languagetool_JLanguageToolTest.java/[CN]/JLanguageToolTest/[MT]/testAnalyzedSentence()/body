{
  final JLanguageTool tool=new JLanguageTool(new English());
  assertEquals("<S> This[this/DT,B-NP-singular|E-NP-singular] is[be/VBZ,B-VP] a[a/DT,B-NP-singular] " + "test??ed[tested/JJ,test/VBD,test/VBN,test??ed/null,I-NP-singular] " + "sentence[sentence/NN,sentence/VB,sentence/VBP,E-NP-singular].[./.,</S>,O]",tool.getAnalyzedSentence("This is a test\u00aded sentence.").toString());
  assertEquals("<S> </S><P/> ",tool.getAnalyzedSentence("\n").toString());
}
