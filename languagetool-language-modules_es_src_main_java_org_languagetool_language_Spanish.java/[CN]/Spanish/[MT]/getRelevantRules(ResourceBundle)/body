{
  return Arrays.asList(new CommaWhitespaceRule(messages),new DoublePunctuationRule(messages),new GenericUnpairedBracketsRule(messages,Arrays.asList("[","(","{","???","??","??","??","??"),Arrays.asList("]",")","}","???","??","??","?","!")),new MorfologikSpanishSpellerRule(messages,this),new UppercaseSentenceStartRule(messages,this),new WordRepeatRule(messages,this),new MultipleWhitespaceRule(messages,this));
}
