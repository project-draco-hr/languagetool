{
  s=s.replaceAll("(?s)\\[\\[Bild:.*?\\]\\]","");
  s=s.replaceAll("(?s)\\[\\[[^\\[]*?\\|(.*?)\\]\\]","$1");
  s=s.replaceAll("(?s)\\[...?:.*?\\]\\]","");
  s=s.replaceAll("(?s)\\[\\[(.*?)\\]\\]","$1");
  s=s.replaceAll("(?s)<math>.*?</math>","X");
  s=s.replaceAll("(?s)<!---.*?--->","");
  s=s.replaceAll("(?si)\\{\\{prettytable\\}\\}.*?\\}","");
  s=s.replaceAll("(?si)\\{\\{Personendaten.*?\\}","");
  s=s.replaceAll("(?si)\\[http://.*?\\]","Quelle");
  s=s.replaceAll("(Quelle\\s*)+","Quelle ");
  s=s.replaceAll("(Quelle\\*\\s*)+","Quelle ");
  s=s.replaceAll("(\\*Quelle\\s*)+","Quelle ");
  s=s.replaceAll("(?si)\\{\\{.*?\\}\\}","");
  s=s.replaceAll("(?si)\\[\\[.*?\\]\\]","");
  s=s.replaceAll("\n\\* ","\n");
  s=s.replaceAll("'''","");
  s=s.replaceAll("''","");
  s=s.replaceAll("<br\\s*/>","\n");
  s=s.replaceAll("\n+","\n");
  s=s.replaceAll("&nbsp;"," ");
  return s;
}
