{
  if (endPos < startPos) {
    throw new RuntimeException("endPos < startPos: " + endPos + " < "+ startPos);
  }
  for (int i=startPos; i < endPos; i++) {
    AnalyzedTokenReadings token=tokens[i];
    for (    AnalyzedToken analyzedToken : token.getReadings()) {
      if (analyzedToken.hasNoTag()) {
        return true;
      }
    }
  }
  return false;
}
