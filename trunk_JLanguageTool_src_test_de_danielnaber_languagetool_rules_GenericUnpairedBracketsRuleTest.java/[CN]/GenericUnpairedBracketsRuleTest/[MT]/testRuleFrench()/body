{
  GenericUnpairedBracketsRule rule=new GenericUnpairedBracketsRule(TestTools.getEnglishMessages(),Language.FRENCH);
  RuleMatch[] matches;
  JLanguageTool langTool=new JLanguageTool(Language.FRENCH);
  matches=rule.match(langTool.getAnalyzedSentence("(Qu'est ce que c'est???)"));
  assertEquals(0,matches.length);
  matches=rule.match(langTool.getAnalyzedSentence("(Qu'est ce que c'est???"));
  assertEquals(1,matches.length);
}
