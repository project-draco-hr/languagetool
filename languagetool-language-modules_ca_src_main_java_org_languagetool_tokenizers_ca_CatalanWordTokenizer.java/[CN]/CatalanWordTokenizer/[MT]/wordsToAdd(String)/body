{
  final List<String> l=new ArrayList<>();
  if (!s.isEmpty()) {
    if (!s.contains("-")) {
      l.add(s);
    }
 else {
      if (!speller.isMisspelled(s)) {
        l.add(s);
      }
 else       if (!speller.isMisspelled(s.replace("l-l","l??l"))) {
        l.add(s);
      }
 else {
        final StringTokenizer st2=new StringTokenizer(s,"-",true);
        while (st2.hasMoreElements()) {
          l.add(st2.nextToken());
        }
      }
    }
  }
  return l;
}
