{
  if (!e.isRegularExpression()) {
    return getLemmas(e.getString());
  }
 else {
    if (isOrRegex(e)) {
      ArrayList<String> lemmas=new ArrayList<String>();
      String[] words=e.getString().split("\\|");
      for (      String word : words) {
        lemmas.addAll(getLemmas(word));
      }
      return lemmas;
    }
    return null;
  }
}
