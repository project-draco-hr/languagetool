{
  assertEmptyMatch("?????? ?????????????? ????????????????????");
  assertEmptyMatch("?????? ???????????? ????????????");
  assertEmptyMatch("?????? 100 100");
  assertEmptyMatch("1.30 3.20 3.20");
  assertEmptyMatch("???? ?? ??.????????????????????????");
  assertEmptyMatch("?????? ?????????? ?????????? ???? ??????????????.");
  assertEmptyMatch("???? ????, ?? ???????? ?? ??????????????...");
  assertEmptyMatch("???????????????????? ???? ????. ????. 3, 7, 18.");
  assertEquals(1,rule.match(langTool.getAnalyzedSentence("?????? ?????? ?????????????? ????????????????????")).length);
  RuleMatch[] match=rule.match(langTool.getAnalyzedSentence("?????????????????? ???????? ?? ?? ???? ????????????????"));
  assertEquals(1,match.length);
  assertEquals(2,match[0].getSuggestedReplacements().size());
}
