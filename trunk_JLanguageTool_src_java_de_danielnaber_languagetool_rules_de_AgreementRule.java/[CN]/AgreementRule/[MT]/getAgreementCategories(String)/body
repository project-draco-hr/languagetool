{
  Set set=new HashSet();
  String[] parts=posTagInformation.split(",");
  for (int i=0; i < parts.length; i++) {
    String allCats=parts[i].trim();
    int delim=allCats.indexOf(" ");
    String cats=allCats.substring(delim);
    if (cats.startsWith("["))     cats=cats.substring(1);
    if (cats.endsWith("]"))     cats=cats.substring(0,cats.length() - 2);
    set.add(cats.trim());
  }
  return set;
}
