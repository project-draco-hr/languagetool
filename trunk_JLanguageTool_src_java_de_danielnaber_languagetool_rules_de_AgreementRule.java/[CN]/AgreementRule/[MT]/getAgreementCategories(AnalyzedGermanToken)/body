{
  Set set=new HashSet();
  List readings=aToken.getReadings();
  for (Iterator iter=readings.iterator(); iter.hasNext(); ) {
    GermanTokenReading reading=(GermanTokenReading)iter.next();
    if (reading.getCasus() == null && reading.getNumerus() == null && reading.getGenus() == null)     continue;
    set.add(reading.getCasus() + "/" + reading.getNumerus()+ "/"+ reading.getGenus());
  }
  return set;
}
