{
  try (PreparedStatement prepSt=conn.prepareStatement("CREATE TABLE feed_matches (" + "  id INT NOT NULL GENERATED ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1)," + "  title VARCHAR(255) NOT NULL,"+ "  rule_id VARCHAR(255) NOT NULL,"+ "  error_context VARCHAR(500) NOT NULL,"+ "  edit_date TIMESTAMP NOT NULL,"+ "  fix_date TIMESTAMP"+ ")")){
    prepSt.executeUpdate();
  }
 catch (  SQLException e) {
    if (!e.getSQLState().equals("X0Y32")) {
      throw e;
    }
  }
}
