{
  final StringBuilder sb=new StringBuilder();
  for (final Iterator<StringBuilder> iter=translations.iterator(); iter.hasNext(); ) {
    final StringBuilder trans=iter.next();
    sb.append('"');
    sb.append(trans.toString());
    sb.append('"');
    if (iter.hasNext()) {
      sb.append(", ");
    }
  }
  return sb.toString();
}
