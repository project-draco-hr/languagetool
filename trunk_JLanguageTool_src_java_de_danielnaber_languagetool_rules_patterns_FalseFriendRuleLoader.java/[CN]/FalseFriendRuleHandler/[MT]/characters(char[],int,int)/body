{
  final String s=new String(buf,offset,len);
  if (inToken && inPattern) {
    elements.append(s);
  }
 else   if (inCorrectExample) {
    correctExample.append(s);
  }
 else   if (inIncorrectExample) {
    incorrectExample.append(s);
  }
 else   if (inTranslation) {
    translation.append(s);
  }
}
