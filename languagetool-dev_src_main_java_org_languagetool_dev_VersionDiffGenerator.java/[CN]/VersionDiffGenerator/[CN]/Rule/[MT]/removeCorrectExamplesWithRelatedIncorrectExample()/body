{
  for (int i=0; i < correct.size(); i++) {
    boolean found=false;
    for (int j=0; j < incorrect.size() && !found; j++) {
      String correctExample=correct.get(i);
      String incorrectExample=incorrect.get(j);
      if (correctExample.startsWith(incorrectExample.substring(0,incorrectExample.indexOf("<b>") + 3)) && correctExample.endsWith(incorrectExample.substring(incorrectExample.indexOf("</b>")))) {
        correct.remove(i);
        i--;
        found=true;
      }
    }
  }
}
