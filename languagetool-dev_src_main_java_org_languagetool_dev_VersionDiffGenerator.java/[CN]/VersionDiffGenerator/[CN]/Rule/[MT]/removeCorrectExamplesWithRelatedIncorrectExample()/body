{
  for (int i=0; i < correct.size(); i++) {
    boolean found=false;
    for (int j=0; j < incorrect.size() && !found; j++) {
      if (correct.get(i).startsWith(incorrect.get(j).substring(0,incorrect.get(j).indexOf("<b>") + 3)) && correct.get(i).endsWith(incorrect.get(j).substring(incorrect.get(j).indexOf("</b>")))) {
        correct.remove(i);
        i--;
        found=true;
      }
    }
  }
}
