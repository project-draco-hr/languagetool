{
  if (taggedTokens != null) {
    int i=0;
    while (i < taggedTokens.length) {
      final String lemma=taggedTokens[i];
      final String[] tagsArr=taggedTokens[i + 1].split("\\+");
      for (      final String currTag : tagsArr) {
        l.add(new AnalyzedToken(word,currTag,lemma));
      }
      i=i + 2;
    }
  }
}
