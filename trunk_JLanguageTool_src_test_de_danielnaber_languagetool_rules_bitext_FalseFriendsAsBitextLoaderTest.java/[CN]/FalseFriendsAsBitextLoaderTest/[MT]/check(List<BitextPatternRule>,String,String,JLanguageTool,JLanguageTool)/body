{
  List<RuleMatch> allMatches=new ArrayList<RuleMatch>();
  for (  BitextPatternRule bRule : bRules) {
    RuleMatch[] matches=match(bRule,src,trg,srcTool,trgTool);
    if (matches != null) {
      for (      RuleMatch match : matches) {
        allMatches.add(match);
      }
    }
  }
  return allMatches;
}
