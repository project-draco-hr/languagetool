{
  boolean doubleI=prevToken.equals("??") && token.equals("??");
  if (doubleI) {
    msg+=" ??????, ??????????????, ?????????? ?? ?????? ???????? ????????????????????.";
  }
  RuleMatch ruleMatch=super.createRuleMatch(prevToken,token,prevPos,pos,msg);
