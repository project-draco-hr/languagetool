{
  ArrayList<String> retList=new ArrayList<>();
  for (  String s : p) {
    if (s.equals("'")) {
      retList.add(s);
    }
 else     if (s.contains("'")) {
      String[] temp=s.replaceAll("'"," ' ").split("\\ +");
      Collections.addAll(retList,temp);
    }
 else {
      retList.add(s);
    }
  }
  return retList.toArray(new String[retList.size()]);
}
