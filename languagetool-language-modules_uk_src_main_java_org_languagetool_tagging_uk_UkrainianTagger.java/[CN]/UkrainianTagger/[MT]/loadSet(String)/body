{
  InputStream is=JLanguageTool.getDataBroker().getFromResourceDirAsStream(path);
  Set<String> result=new HashSet<>();
  try (Scanner scanner=new Scanner(is,"UTF-8")){
    while (scanner.hasNextLine()) {
      String line=scanner.nextLine();
      result.add(line);
    }
    return result;
  }
 }
