{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  RuleMatch other=(RuleMatch)o;
  return Objects.equals(rule.getId(),other.rule.getId()) && Objects.equals(offsetPosition,other.offsetPosition) && Objects.equals(message,other.message)&& Objects.equals(suggestedReplacements,other.suggestedReplacements);
}
