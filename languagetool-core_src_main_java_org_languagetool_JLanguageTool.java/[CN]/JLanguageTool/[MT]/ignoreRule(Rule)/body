{
  if (disabledRules.contains(rule.getId())) {
    return true;
  }
  if (rule.isDefaultOff() && !enabledRules.contains(rule.getId())) {
    return true;
  }
  Category category=rule.getCategory();
  if (category != null && disabledCategories.contains(category.getName())) {
    return true;
  }
  return false;
}
