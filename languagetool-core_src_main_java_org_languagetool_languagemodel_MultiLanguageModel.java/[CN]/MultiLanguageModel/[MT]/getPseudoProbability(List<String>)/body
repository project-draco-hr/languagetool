{
  double prob=0;
  float coverage=0;
  long occurrences=0;
  for (  LanguageModel lm : lms) {
    Probability pProb=lm.getPseudoProbability(context);
    prob+=pProb.getProb();
    coverage+=pProb.getProb();
    occurrences+=pProb.getOccurrences();
  }
  return new Probability(prob,coverage / lms.size(),occurrences);
}
