{
  ArrayList<String> returnList=new ArrayList<String>();
  Scanner in=null;
  InputStream is=null;
  try {
    is=JLanguageTool.getDataBroker().getFromResourceDirAsStream(filename);
    in=new Scanner(is);
    while (in.hasNextLine()) {
      returnList.add(in.nextLine());
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    in.close();
  }
  return returnList;
}
