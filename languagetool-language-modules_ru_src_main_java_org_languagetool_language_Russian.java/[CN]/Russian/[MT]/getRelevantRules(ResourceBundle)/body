{
  return Arrays.asList(new CommaWhitespaceRule(messages),new DoublePunctuationRule(messages),new UppercaseSentenceStartRule(messages,this),new MorfologikRussianSpellerRule(messages,this),new WordRepeatRule(messages,this),new MultipleWhitespaceRule(messages,this),new RussianUnpairedBracketsRule(messages,this),new RussianCompoundRule(messages),new RussianSimpleReplaceRule(messages),new RussianWordRepeatRule(messages));
}
