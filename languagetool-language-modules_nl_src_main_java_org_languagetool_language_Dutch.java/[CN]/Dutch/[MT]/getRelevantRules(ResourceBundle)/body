{
  return Arrays.asList(new CommaWhitespaceRule(messages),new DoublePunctuationRule(messages),new GenericUnpairedBracketsRule(messages,this),new UppercaseSentenceStartRule(messages,this),new DutchSpellerRule(messages,this),new MultipleWhitespaceRule(messages,this),new CompoundRule(messages),new DutchWrongWordInContextRule(messages));
}
