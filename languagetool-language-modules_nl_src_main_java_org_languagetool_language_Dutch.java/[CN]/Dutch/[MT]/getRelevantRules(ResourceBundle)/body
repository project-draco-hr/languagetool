{
  return Arrays.asList(new CommaWhitespaceRule(messages),new DoublePunctuationRule(messages),new GenericUnpairedBracketsRule(messages,Arrays.asList("[","(","{","???","\u2039","\u201c","\u201e"),Arrays.asList("]",")","}","???","\u203a","\u201d","\u201d")),new UppercaseSentenceStartRule(messages,this),new MorfologikDutchSpellerRule(messages,this),new MultipleWhitespaceRule(messages,this),new CompoundRule(messages),new DutchWrongWordInContextRule(messages),new SimpleReplaceRule(messages));
}
