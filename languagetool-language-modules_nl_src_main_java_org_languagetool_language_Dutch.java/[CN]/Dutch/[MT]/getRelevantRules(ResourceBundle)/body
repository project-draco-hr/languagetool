{
  return Arrays.asList(new CommaWhitespaceRule(messages),new DoublePunctuationRule(messages),new GenericUnpairedBracketsRule(messages,Arrays.asList("[","(","{","???","???","???","???"),Arrays.asList("]",")","}","???","???","???","???")),new UppercaseSentenceStartRule(messages,this),new MorfologikDutchSpellerRule(messages,this),new MultipleWhitespaceRule(messages,this),new CompoundRule(messages),new DutchWrongWordInContextRule(messages),new SimpleReplaceRule(messages));
}
