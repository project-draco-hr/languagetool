{
  if ("\u0002".equals(str) || "\u0001".equals(str)) {
    return false;
  }
  final String trimStr=str.trim();
  if (isEmpty(trimStr)) {
    return true;
  }
  if (trimStr.length() == 1) {
    if ("\u200B".equals(str)) {
      return true;
    }
    return java.lang.Character.isWhitespace(trimStr.charAt(0));
  }
  return false;
}
