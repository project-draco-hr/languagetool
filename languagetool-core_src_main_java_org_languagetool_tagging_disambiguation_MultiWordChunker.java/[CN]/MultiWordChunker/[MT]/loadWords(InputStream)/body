{
  final List<String> lines=new ArrayList<String>();
  final Scanner scanner=new Scanner(stream,"UTF-8");
  try {
    while (scanner.hasNextLine()) {
      final String line=scanner.nextLine().trim();
      if (line.length() < 1) {
        continue;
      }
      if (line.charAt(0) == '#') {
        continue;
      }
      lines.add(line);
    }
  }
  finally {
    scanner.close();
  }
  return lines;
}
