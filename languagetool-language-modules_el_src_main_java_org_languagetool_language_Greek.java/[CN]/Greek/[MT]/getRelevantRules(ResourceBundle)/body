{
  return Arrays.asList(new CommaWhitespaceRule(messages),new DoublePunctuationRule(messages),new GreekUnpairedBracketsRule(messages,this),new LongSentenceRule(messages),new MorfologikGreekSpellerRule(messages,this),new UppercaseSentenceStartRule(messages,this),new MultipleWhitespaceRule(messages,this),new WordRepeatBeginningRule(messages,this),new WordRepeatRule(messages,this));
}
