{
  return Arrays.asList(new CommaWhitespaceRule(messages,Example.wrong("???? ?????????? ?????????????? ??????????????????<marker> ,</marker> ?????????? ?????????????????? ?????? ??????????????."),Example.fixed("???? ?????????? ?????????????? ??????????????????<marker>,</marker> ?????????? ?????????????????? ?????? ??????????????.")),new DoublePunctuationRule(messages),new GenericUnpairedBracketsRule("EL_UNPAIRED_BRACKETS",messages,Arrays.asList("[","(","{","???","\"","??"),Arrays.asList("]",")","}","???","\"","??")),new LongSentenceRule(messages),new MorfologikGreekSpellerRule(messages,this),new UppercaseSentenceStartRule(messages,this,Example.wrong("?? ???????????? ?????????? ???????????? ????????????. <marker>??????????????</marker> ???? ?????????? ???????? ????????????????."),Example.fixed("?? ???????????? ?????????? ???????????? ????????????. <marker>??????????????</marker> ???? ?????????? ???????? ????????????????.")),new MultipleWhitespaceRule(messages,this),new WordRepeatBeginningRule(messages,this),new WordRepeatRule(messages,this),new NumeralStressRule(messages));
