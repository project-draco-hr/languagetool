{
  return Arrays.asList(new CommaWhitespaceRule(messages),new DoublePunctuationRule(messages),new GenericUnpairedBracketsRule("EL_UNPAIRED_BRACKETS",messages,Arrays.asList("[","(","{","???","\"","??"),Arrays.asList("]",")","}","???","\"","??")),new LongSentenceRule(messages),new MorfologikGreekSpellerRule(messages,this),new UppercaseSentenceStartRule(messages,this),new MultipleWhitespaceRule(messages,this),new WordRepeatBeginningRule(messages,this),new WordRepeatRule(messages,this));
}
