{
  this.stringToken=token;
  testString=!StringTools.isEmpty(stringToken);
  if (testString && stringRegExp) {
    String regToken=stringToken;
    if (!caseSensitive) {
      regToken=CASE_INSENSITIVE + stringToken;
    }
    if (!"\\0".equals(token)) {
      p=Pattern.compile(regToken);
    }
  }
}
