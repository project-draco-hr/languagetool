{
  this.posToken=posToken;
  this.posNegation=negation;
  posRegExp=regExp;
  if (posRegExp) {
    pPos=Pattern.compile(posToken);
    if (mPos == null) {
      mPos=pPos.matcher(UNKNOWN_TAG);
    }
 else {
      mPos.reset(UNKNOWN_TAG);
    }
    posUnknown=mPos.matches();
  }
 else {
    posUnknown=UNKNOWN_TAG.equals(posToken);
  }
}
