{
  if (dictLookup == null) {
    dictLookup=(DictionaryLookup)loadDictionary();
  }
  if (hasPosTag(word)) {
    final String[] splitWord=word.split("/");
    if (Pattern.matches(splitWord[1],posTag)) {
      return true;
    }
    return false;
  }
  final List<WordData> lwd=dictLookup.lookup(word);
  for (  WordData wd : lwd) {
    if (wd.getTag().toString().equals(posTag)) {
      return true;
    }
  }
  return false;
}
