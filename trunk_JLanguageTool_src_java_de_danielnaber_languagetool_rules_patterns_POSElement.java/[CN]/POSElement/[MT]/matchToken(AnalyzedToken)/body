{
  boolean match=false;
  for (int i=0; i < tokens.length; i++) {
    if (tokens[i].equals(token.getPOSTag())) {
      match=true;
      break;
    }
  }
  if (exceptions != null) {
    for (int i=0; i < exceptions.length; i++) {
      boolean tmpMatch;
      if (caseSensitive)       tmpMatch=exceptions[i].equals(token.getToken());
 else       tmpMatch=exceptions[i].equalsIgnoreCase(token.getToken());
      if (tmpMatch) {
        return false;
      }
    }
  }
  return match;
}
