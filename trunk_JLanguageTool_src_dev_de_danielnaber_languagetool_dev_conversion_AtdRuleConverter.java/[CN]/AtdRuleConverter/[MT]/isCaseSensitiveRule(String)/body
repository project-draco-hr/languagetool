{
  boolean caseSensitive=false;
  String[] splitPattern=pattern.split("\\ +");
  for (  String s : splitPattern) {
    if (s.equals("0BEGIN.0") || s.equals("0END.0")) {
      continue;
    }
    String[] splitS=s.split("/");
    if (uppercase.matcher(splitS[0]).find()) {
      caseSensitive=true;
    }
  }
  return caseSensitive;
}
