{
  Matcher alreadyOff=defaultOffRegex.matcher(rule);
  if (alreadyOff.find()) {
    return rule;
  }
 else {
    Matcher ruleHeader=ruleHeaderRegex.matcher(rule);
    if (!ruleHeader.find()) {
      System.out.println();
    }
    String newRule=rule.replace(ruleHeader.group(),ruleHeader.group().concat(defaultOffString));
    return newRule;
  }
}
