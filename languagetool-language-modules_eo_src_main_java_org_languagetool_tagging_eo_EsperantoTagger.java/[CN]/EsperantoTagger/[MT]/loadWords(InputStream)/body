{
  final Set<String> words=new HashSet<>();
  try (InputStreamReader isr=new InputStreamReader(stream,"UTF-8");BufferedReader br=new BufferedReader(isr)){
    String line;
    while ((line=br.readLine()) != null) {
      line=line.trim();
      if (line.isEmpty() || line.charAt(0) == '#') {
        continue;
      }
      words.add(line);
    }
  }
   return words;
}
