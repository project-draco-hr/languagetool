{
  return Arrays.asList(new CommaWhitespaceRule(messages,Example.wrong("???? ?????????????? ????????????<marker> ,</marker> ???????????? ?? ??????????????."),Example.fixed("???? ?????????????? ????????????<marker>,</marker> ???????????? ?? ??????????????")),new MorfologikUkrainianSpellerRule(messages,this),new MixedAlphabetsRule(messages),new MultipleWhitespaceRule(messages,this),new UkrainianWordRepeatRule(messages,this),new SimpleReplaceRule(messages),new SimpleReplaceSoftRule(messages),new TokenAgreementRule(messages),new TokenInflectionAgreementRule(messages),new HiddenCharacterRule(messages));
}
