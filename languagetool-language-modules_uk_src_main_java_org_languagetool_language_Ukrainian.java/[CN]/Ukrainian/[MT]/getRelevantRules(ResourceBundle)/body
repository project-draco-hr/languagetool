{
  return Arrays.asList(new CommaWhitespaceRule(messages),new MorfologikUkrainianSpellerRule(messages,this),new MixedAlphabetsRule(messages),new MultipleWhitespaceRule(messages,this),new UkrainianWordRepeatRule(messages,this),new SimpleReplaceRule(messages),new SimpleReplaceSoftRule(messages),new TokenAgreementRule(messages),new HiddenCharacterRule(messages));
}
