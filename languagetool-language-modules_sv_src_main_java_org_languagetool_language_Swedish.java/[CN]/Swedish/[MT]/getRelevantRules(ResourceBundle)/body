{
  return Arrays.asList(new CommaWhitespaceRule(messages),new DoublePunctuationRule(messages),new GenericUnpairedBracketsRule(messages),new HunspellRule(messages,this),new UppercaseSentenceStartRule(messages,this),new WordRepeatRule(messages,this),new MultipleWhitespaceRule(messages,this),new CompoundRule(messages));
}
