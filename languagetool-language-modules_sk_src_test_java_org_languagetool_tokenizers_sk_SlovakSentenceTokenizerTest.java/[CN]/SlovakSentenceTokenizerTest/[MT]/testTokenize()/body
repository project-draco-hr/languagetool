  testSplit(new String[]{"Ak?? pr??platky zamestnancovi (napr. za nad??as) stanovuje Z??konn??k pr??ce?"});
  testSplit(new String[]{"Po??as nepr??tomnosti zastupuje MUDr. Marianna Krup??ov??."});
  testSplit(new String[]{"Starovek?? Egypt vznikol okolo r. 3150 p.n.l. (tzn. 3150 pred Kr.). ","A zanikol v r. 31 pr. Kr."});
  testSplit(new String[]{"Temperatura wody w systemie wynosi 30??C.","W sk??ad obiegu otwartego wchodzi zbiornik i armatura."});
  testSplit(new String[]{"Zabudowano kolumny o d??ugo??ci 45 m. ","Woda z uj??cia jest dostarczana do zak??adu."});
