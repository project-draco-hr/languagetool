{
  final List<String> l=new ArrayList<String>();
  final StringTokenizer st=new StringTokenizer(text,"\u0020\u00A0\u2000\u2001\u2002\u2003\u2004\u2005\u2006" + "\u2007\u2008\u2009\u200A\u200B\u202F\u205F\u3000\uFEFF" + ",.;()[]{}!?:/\\\"'??????????????????????????\t\n",true);
  while (st.hasMoreElements()) {
    l.add(st.nextToken());
  }
  return l;
}
