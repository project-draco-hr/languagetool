{
  final HunspellRule rule=new HunspellRule(TestTools.getMessages("German"),Language.GERMANY_GERMAN);
  final JLanguageTool langTool=new JLanguageTool(Language.GERMAN);
  assertEquals(0,rule.match(langTool.getAnalyzedSentence("Der Waschmaschinentestversuch")).length);
  assertEquals(0,rule.match(langTool.getAnalyzedSentence("Der ??u??ere ??belt??ter.")).length);
  assertEquals(1,rule.match(langTool.getAnalyzedSentence("Der ??ussere ??belt??ter.")).length);
  assertEquals(1,rule.match(langTool.getAnalyzedSentence("Der Waschmaschinentestversuch orkt")).length);
}
