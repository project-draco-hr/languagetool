{
  try {
    final ResourceBundle bundle=ResourceBundle.getBundle("de.danielnaber.languagetool.MessagesBundle");
    final ResourceBundle fallbackBundle=ResourceBundle.getBundle("de.danielnaber.languagetool.MessagesBundle",Locale.ENGLISH);
    return new ResourceBundleWithFallback(bundle,fallbackBundle);
  }
 catch (  final MissingResourceException e) {
    return ResourceBundle.getBundle("de.danielnaber.languagetool.MessagesBundle",Locale.ENGLISH);
  }
}
