def __init__(self):
    tag_lines = re.split('\n', self.TAG_STRING)
    self.tags = []
    for tag_line in tag_lines:
        tag_line = tag_line.strip()
        parts = re.split('\\s+', tag_line)
        short_tag = parts[0]
        tag_exp = str.join(' ', parts[1:])
        self.tags.append((short_tag, tag_exp))
    return
