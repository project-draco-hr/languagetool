{
  URL fsaURL=JLanguageTool.getDataBroker().getFromResourceDirAsUrl("de/hunspell/de_DE.dict");
  Dictionary dictionary=Dictionary.read(fsaURL);
  Speller speller=new Speller(dictionary,2);
  List<String> input=Arrays.asList(("Abenteur Abh??ngikeit abzuschliessen agerufen Aktiviti??ten Aktzeptanz " + "Algorhitmus Algoritmus aliiert allgmein Amtsitz ??nlich Anstoss atakieren begr??sst Bezeichnug chinesiche " + "dannach Frima Fahrad Geba??de gesammt "+ "ger??gelt Aufjedenfall ivh h??llt da?? woeder oderso anwalt").split(" "));
  for (  String word : input) {
    check(word,speller);
  }
}
