def makeChecker():
    grammar_cfg = None
    falsefriends_cfg = None
    words_cfg = None
    textlanguage = None
    mothertongue = None
    max_sentence_length = None
    checker = TextChecker.TextChecker(grammar_cfg, falsefriends_cfg, words_cfg, textlanguage, mothertongue, max_sentence_length)
