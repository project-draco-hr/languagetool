{
  if (field == term.field()) {
    String searchText=term.text();
    if ((isPOS && !searchText.startsWith(LanguageToolFilter.POS_PREFIX)) || (!isPOS && searchText.startsWith(LanguageToolFilter.POS_PREFIX))) {
      return false;
    }
    if (isPOS) {
      searchText=searchText.replaceFirst(LanguageToolFilter.POS_PREFIX,"");
    }
    if (searchText.startsWith(pre)) {
      return regexImpl.match(searchText);
    }
  }
  endEnum=true;
  return false;
}
