{
  char d=(char)0;
  while ((lpindex < inArray.length) && inArray[lpindex] != a && inArray[lpindex] != b) {
    if (ISNL(lpindex)) {
      ++d;
    }
    if (inArray[lpindex] == '#' && !ISESC(lpindex)) {
      d+=SKIPLN_N();
      lpindex--;
    }
    if (!ISSPACE(lpindex)) {
      break;
    }
    ++lpindex;
  }
  return d;
}
