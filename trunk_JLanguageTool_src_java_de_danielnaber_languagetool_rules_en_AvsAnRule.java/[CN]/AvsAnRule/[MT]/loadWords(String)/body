{
  FileReader fr=null;
  BufferedReader br=null;
  Set set=new HashSet();
  try {
    fr=new FileReader(filename);
    br=new BufferedReader(fr);
    String line;
    while ((line=br.readLine()) != null) {
      line=line.trim();
      if (line.startsWith("#"))       continue;
      set.add(line.toLowerCase());
    }
  }
  finally {
    if (br != null)     br.close();
    if (fr != null)     fr.close();
  }
  return set;
}
