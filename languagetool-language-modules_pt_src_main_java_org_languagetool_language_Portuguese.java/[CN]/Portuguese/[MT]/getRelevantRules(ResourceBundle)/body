{
  return Arrays.asList(new CommaWhitespaceRule(messages),new DoublePunctuationRule(messages),new GenericUnpairedBracketsRule(messages),new HunspellNoSuggestionRule(messages,this),new UppercaseSentenceStartRule(messages,this),new WordRepeatRule(messages,this),new MultipleWhitespaceRule(messages,this),new SentenceWhitespaceRule(messages),new PreReformPortugueseCompoundRule(messages),new PortugueseReplaceRule(messages));
}
