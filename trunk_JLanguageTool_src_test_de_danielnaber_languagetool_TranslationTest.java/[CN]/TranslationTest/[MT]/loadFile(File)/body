{
  List<String> l=new ArrayList<String>();
  FileReader fr=null;
  BufferedReader br=null;
  try {
    fr=new FileReader(file);
    br=new BufferedReader(fr);
    String line;
    while ((line=br.readLine()) != null) {
      l.add(line);
    }
  }
  finally {
    if (br != null)     br.close();
    if (fr != null)     fr.close();
  }
  return l;
}
