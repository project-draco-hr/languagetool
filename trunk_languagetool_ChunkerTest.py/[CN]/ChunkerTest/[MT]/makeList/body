def makeList(self, s):
    parts = re.split('(\\s+)', s)
    l = []
    for part in parts:
        word = None
        word_norm = None
        tag = None
        pair = re.split('/', part)
        if (len(pair) == 2):
            (word, tag) = pair
            word_norm = word
        else:
            word = pair[0]
        l.append((word, word_norm, tag))
    return l
