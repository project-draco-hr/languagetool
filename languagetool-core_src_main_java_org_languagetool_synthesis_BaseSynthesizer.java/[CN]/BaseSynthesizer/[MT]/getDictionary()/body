{
  Dictionary dict=this.dictionary;
  if (dict == null) {
synchronized (this) {
      dict=this.dictionary;
      if (dict == null) {
        final URL url=JLanguageTool.getDataBroker().getFromResourceDirAsUrl(resourceFileName);
        this.dictionary=dict=Dictionary.read(url);
      }
    }
  }
  return dict;
}
