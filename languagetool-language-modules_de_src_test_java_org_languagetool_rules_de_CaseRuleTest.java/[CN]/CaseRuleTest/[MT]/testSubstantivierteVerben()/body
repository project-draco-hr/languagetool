  assertEquals(0,rule.match(langTool.getAnalyzedSentence("Er klopfte mit Zittern und Zagen an.")).length);
  assertEquals(0,rule.match(langTool.getAnalyzedSentence("Ich nehme die Tabletten auf Anraten meiner ??rztin.")).length);
