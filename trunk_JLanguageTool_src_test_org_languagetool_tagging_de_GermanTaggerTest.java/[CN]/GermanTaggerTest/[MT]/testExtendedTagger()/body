{
  GermanTagger tagger=new GermanTagger();
  assertEquals("Ku??[SUB:AKK:SIN:MAS, SUB:DAT:SIN:MAS, SUB:NOM:SIN:MAS]",tagger.lookup("Ku??").toSortedString());
  assertEquals("Kuss[SUB:AKK:SIN:MAS, SUB:DAT:SIN:MAS, SUB:NOM:SIN:MAS]",tagger.lookup("Kuss").toSortedString());
  assertEquals("Ha??[SUB:AKK:SIN:MAS, SUB:DAT:SIN:MAS, SUB:NOM:SIN:MAS]",tagger.lookup("Ha??").toSortedString());
  assertEquals("Hass[SUB:AKK:SIN:MAS, SUB:DAT:SIN:MAS, SUB:NOM:SIN:MAS]",tagger.lookup("Hass").toSortedString());
  assertEquals("mu??[VER:MOD:1:SIN:PR??, VER:MOD:3:SIN:PR??]",tagger.lookup("mu??").toSortedString());
  assertEquals("muss[VER:MOD:1:SIN:PR??, VER:MOD:3:SIN:PR??]",tagger.lookup("muss").toSortedString());
}
