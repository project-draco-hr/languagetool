{
  List l=new ArrayList();
  for (int i=0; i < tokens.length; i++) {
    AnalyzedTokenReadings token=tokens[i];
    if (!token.getAnalyzedToken(0).isWhitespace() || (token.getAnalyzedToken(0).getPOSTag() != null && token.getAnalyzedToken(0).getPOSTag().equals(JLanguageTool.SENTENCE_START_TAGNAME))) {
      l.add(token);
    }
  }
  return (AnalyzedTokenReadings[])l.toArray(new AnalyzedTokenReadings[0]);
}
