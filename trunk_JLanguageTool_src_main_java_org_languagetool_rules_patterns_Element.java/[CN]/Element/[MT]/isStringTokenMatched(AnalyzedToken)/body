{
  final String testToken=getTestToken(token);
  if (stringRegExp) {
    if (m == null) {
      m=p.matcher(testToken);
    }
 else {
      m.reset(testToken);
    }
    return m.matches();
  }
  if (caseSensitive) {
    return stringToken.equals(testToken);
  }
  return stringToken.equalsIgnoreCase(testToken);
}
