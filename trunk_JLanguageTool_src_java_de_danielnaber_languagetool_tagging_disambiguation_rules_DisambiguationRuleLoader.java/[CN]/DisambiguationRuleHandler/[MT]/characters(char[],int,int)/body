{
  final String s=new String(buf,offset,len);
  if (inException) {
    exceptions.append(s);
  }
 else   if (inToken && inPattern) {
    elements.append(s);
  }
 else   if (inMatch) {
    match.append(s);
  }
 else   if (inWord) {
    wd.append(s);
  }
 else   if (inDisambiguation) {
    disamb.append(s);
  }
 else   if (inExample) {
    example.append(s);
  }
}
